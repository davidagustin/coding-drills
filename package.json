    "isomorphic-dompurify": "^2.35.0",
    "lucide-react": "^0.563.0",
    "monaco-editor": "^0.55.1",
    "motion": "^12.29.0",
    "next": "16.1.4",
    "nuqs": "^2.8.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
